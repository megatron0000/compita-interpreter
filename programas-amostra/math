program Math {

global:
  int globalVar;

functions:

void Search(int vector) {
  local:
    char localVar;
  
  statements:
    localVar <- 'a';
    globalVar <- 1 <= 2;
    localVar <- 1 + (2 * localVar) / 3 % 5 + ~1;
    globalVar <- 1.1234567898765432123456789;
    globalVar <- 1.12345678987654321;
}

}